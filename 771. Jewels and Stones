class Solution {
public:
    int numJewelsInStones(string J, string S) {
        int k = 0;
        for(int i = 0; i < J.size(); i++){
            for (int j = 0; j < S.size(); j++ ){
                if (S[j] ==  J[i]){
                    k = k+1;
                }
            }
        }
        return k;
        
    }
};
